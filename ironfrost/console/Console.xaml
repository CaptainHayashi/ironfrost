<Window x:Name="console" x:Class="ironfrost.Console"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ironfrost"
        mc:Ignorable="d"
        Title="{Binding ClientName}" Height="350" Width="525">
    <DockPanel Name="consolePanel">
        <ContentControl Name="consoleSnapin" Content="{Binding SnapIn}" />
    </DockPanel>
</Window>
